{
  "family": "springboot-backend-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "springboot-backend",
      "image": "REPLACE_IMAGE_URI",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
